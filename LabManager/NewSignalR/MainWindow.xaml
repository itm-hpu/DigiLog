<Window x:Class="NewSignalR.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewSignalR"
        Title="MainWindow" Height="563.143" Width="800">
    <Grid>
        <Button x:Name="btnDebbug" Content="Debbug" HorizontalAlignment="Left" Margin="707,216,0,0" VerticalAlignment="Top" Width="75" Click="BtnDebbug_Click"/>
        <Label Content="Tag ID" HorizontalAlignment="Left" Margin="506,13,0,0" VerticalAlignment="Top" Width="55"/>
        <TextBox x:Name="txtTagID" HorizontalAlignment="Left" Height="81" Margin="561,17,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="125"/>
        <ComboBox x:Name="cmbTagID1" HorizontalAlignment="Left" Margin="10,106,0,0" VerticalAlignment="Top" Width="87"/>
        <ComboBox x:Name="cmbTagID2" HorizontalAlignment="Left" Margin="10,200,0,0" VerticalAlignment="Top" Width="87"/>
        <ComboBox x:Name="cmbTagID3" HorizontalAlignment="Left" Margin="10,294,0,0" VerticalAlignment="Top" Width="87"/>
        <Button x:Name="btnStart" Content="Start" HorizontalAlignment="Left" Margin="707,107,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.162,0.264" Click="BtnStart_Click"/>
        <Label Content="Server" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="87"/>
        <Label Content="UserName" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="87"/>
        <Label Content="UserPassword" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" Width="87"/>
        <TextBox x:Name="txtServer" HorizontalAlignment="Left" Height="26" Margin="105,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="313"/>
        <TextBox x:Name="txtUserName" HorizontalAlignment="Left" Height="26" Margin="105,41,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="313"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Height="26" Margin="105,72,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="313"/>
        <Button x:Name="btnCheck" Content="CheckID" HorizontalAlignment="Left" Margin="423,10,0,0" VerticalAlignment="Top" Width="62" Height="88" Click="btnCheck_Click"/>
        <Button x:Name="BtnClear" Content="Clear" HorizontalAlignment="Left" Margin="707,159,0,0" VerticalAlignment="Top" Width="75" Click="BtnClear_Click"/>
        <Button x:Name="btnStop" Content="Stop" HorizontalAlignment="Left" Margin="707,133,0,0" VerticalAlignment="Top" Width="75" Click="BtnStop_Click"/>
        <Button x:Name="btnDistance" Content="Distance" HorizontalAlignment="Left" Margin="707,400,0,0" VerticalAlignment="Top" Width="75" Click="BtnDistance_Click"/>
        <ComboBox x:Name="cmbObjectForDistance" HorizontalAlignment="Left" Margin="10,400,0,0" VerticalAlignment="Top" Width="87"/>
        <TextBox x:Name="txtObjectForDistance" HorizontalAlignment="Left" VerticalScrollBarVisibility="Visible" Height="114" Margin="105,399,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="581"/>
        <Label Content="max age (min)" HorizontalAlignment="Left" Margin="13,464,0,0" VerticalAlignment="Top" Width="87"/>
        <TextBox x:Name="txtmax_age" HorizontalAlignment="Left" Height="23" Margin="13,490,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="87" TextDecorations="{x:Null}"/>
        <ComboBox x:Name="cmbAggregation" HorizontalAlignment="Left" Margin="10,437,0,0" VerticalAlignment="Top" Width="87" RenderTransformOrigin="0.048,-0.138"/>
        <ListBox x:Name="listbox1" ItemsSource="{Binding positionList1}" ScrollViewer.VerticalScrollBarVisibility="Visible" HorizontalAlignment="Stretch" Height="89" Margin="105,107,108.286,0" VerticalAlignment="Top">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Grid >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Label Grid.Column="0" Content="{Binding ObjectId}" MinWidth="30"/>
                        <Label Grid.Column="1" Content="{Binding Timestamp}" ContentStringFormat="yyyy-MM-dd HH:mm:ss" MinWidth="30"/>
                        <Label Grid.Column="2" Content="{Binding X}" MinWidth="50"/>
                        <Label Grid.Column="3" Content="{Binding Y}" MinWidth="50"/>
                        <Label Grid.Column="4" Content="{Binding Zone}" MinWidth="30"/>
                        <Label Grid.Column="5" Content="{Binding Longitude}" MinWidth="80"/>
                        <Label Grid.Column="6" Content="{Binding Latitude}" MinWidth="80"/>
                    </Grid>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <ListBox x:Name="listbox2" ItemsSource="{Binding positionList2}" ScrollViewer.VerticalScrollBarVisibility="Visible" HorizontalAlignment="Stretch" Height="89" Margin="105,201,108.286,0" VerticalAlignment="Top">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Grid >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Label Grid.Column="0" Content="{Binding ObjectId}" MinWidth="30"/>
                        <Label Grid.Column="1" Content="{Binding Timestamp}" ContentStringFormat="yyyy-MM-dd HH:mm:ss" MinWidth="30"/>
                        <Label Grid.Column="2" Content="{Binding X}" MinWidth="50"/>
                        <Label Grid.Column="3" Content="{Binding Y}" MinWidth="50"/>
                        <Label Grid.Column="4" Content="{Binding Zone}" MinWidth="30"/>
                        <Label Grid.Column="5" Content="{Binding Longitude}" MinWidth="80"/>
                        <Label Grid.Column="6" Content="{Binding Latitude}" MinWidth="80"/>
                    </Grid>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <ListBox x:Name="listbox3" ItemsSource="{Binding positionList3}" ScrollViewer.VerticalScrollBarVisibility="Visible" HorizontalAlignment="Stretch" Height="89" Margin="105,294,108.286,0" VerticalAlignment="Top">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Grid >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Label Grid.Column="0" Content="{Binding ObjectId}" MinWidth="30"/>
                        <Label Grid.Column="1" Content="{Binding Timestamp}" ContentStringFormat="yyyy-MM-dd HH:mm:ss" MinWidth="30"/>
                        <Label Grid.Column="2" Content="{Binding X}" MinWidth="50"/>
                        <Label Grid.Column="3" Content="{Binding Y}" MinWidth="50"/>
                        <Label Grid.Column="4" Content="{Binding Zone}" MinWidth="30"/>
                        <Label Grid.Column="5" Content="{Binding Longitude}" MinWidth="80"/>
                        <Label Grid.Column="6" Content="{Binding Latitude}" MinWidth="80"/>
                    </Grid>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>
