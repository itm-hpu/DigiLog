<Window x:Class="NewSignalR.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewSignalR"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtLog11" HorizontalAlignment="Left" VerticalScrollBarVisibility="Visible" Height="89" Margin="105,91,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="581"/>
        <Button x:Name="btnUpdate" Content="Update" HorizontalAlignment="Left" Margin="707,91,0,0" VerticalAlignment="Top" Width="75" Click="BtnUpdate_Click"/>
        <TextBox x:Name="txtLog12" HorizontalAlignment="Left" Height="89" VerticalScrollBarVisibility="Visible" Margin="105,185,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="581"/>
        <TextBox x:Name="txtLog13" HorizontalAlignment="Left" Height="89" VerticalScrollBarVisibility="Visible" Margin="105,278,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="581"/>
        <Label Content="Tag ID" HorizontalAlignment="Left" Margin="10,18,0,0" VerticalAlignment="Top" Width="84"/>
        <TextBox x:Name="txtTagID" HorizontalAlignment="Left" Height="68" Margin="105,18,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cmbTagID1" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Width="87"/>
        <ComboBox x:Name="cmbTagID2" HorizontalAlignment="Left" Margin="10,184,0,0" VerticalAlignment="Top" Width="87"/>
        <ComboBox x:Name="cmbTagID3" HorizontalAlignment="Left" Margin="10,278,0,0" VerticalAlignment="Top" Width="87"/>
        <Button x:Name="btnStart" Content="Start" HorizontalAlignment="Left" Margin="253,17,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.162,0.264" Click="BtnStart_Click"/>
    </Grid>
</Window>
